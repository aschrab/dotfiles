{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.rulers": [80],
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.testClass": ["mvn", "test", "-Dtest=${class}", "-DfailIfNoTests=false"],
    "java.testMethod": ["mvn", "test", "-Dtest=${class}#${method}", "-DfailIfNoTests=false"],
    "java.debugTestMethod": ["mvn", "test", "-Dtest=${class}#${method}", "-DfailIfNoTests=false", "-Dmaven.surefire.debug"],
    "editor.renderWhitespace": "all",
    "git.allowForcePush": true,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.autorefresh": false,
    "vim.camelCaseMotion.enable": true,
    "vim.incsearch": true,
    "vim.leader": ",",
    "vim.insertModeKeyBindingsNonRecursive": [
        { "before": ["j", "l"], "after": ["<Esc>"]}
    ],
    "vim.insertModeKeyBindings": [
        { "before": [ "<C-d>" ], "commands": [ "editor.action.outdentLines" ] },
    ],
    "vim.normalModeKeyBindings": [
        { "before": ["<C-h>"], "after": ["<C-w>", "h"]},
        { "before": ["<C-j>"], "after": ["<C-w>", "j"]},
        { "before": ["<C-k>"], "after": ["<C-w>", "k"]},
        { "before": ["<C-l>"], "after": ["<C-w>", "l"]},
        { "before": ["[", "c"], "commands": ["workbench.action.editor.previousChange"]},
        { "before": ["]", "c"], "commands": ["workbench.action.editor.nextChange"]},
        { "before": ["[", "k"], "commands": ["editor.action.marker.prev"] },
        { "before": ["]", "k"], "commands": ["editor.action.marker.next"] }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        { "before": [ ">" ], "commands": [ "editor.action.indentLines" ] },
        { "before": [ "<" ], "commands": [ "editor.action.outdentLines" ] },
        { "before": [ "p", ], "after": [ "p", "g", "v", "y" ] }
    ],
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.showcmd": true,
    "vim.showmodename": true,
    "vim.foldfix": true,
    "vim.startInInsertMode": false,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "editor.cursorBlinking": "solid",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "editor.lineNumbers": "relative",
    "editor.overviewRulerBorder": false,
    "editor.renderControlCharacters": true,
    "editor.renderFinalNewline": false,
    "editor.renderLineHighlight": "gutter",
    "editor.tabCompletion": "onlySnippets",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.suggest.maxVisibleSuggestions": 7,
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.statusBar.feedback.visible": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.useSplitJSON": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
