if ( $LIST )
{
  dotlock "$HOME/.mail/dup/$LIST.lock" {
    `reformail -D 8000 $HOME/.mail/dup/$LIST`
    if ( $RETURNCODE == 0 )
    {
      FOLDER=dups/
      include $DELIVER
    }
  }
}

if ( $SUBJ )
{
  SUBJECT=`reformail -xSubject: | sed -e "s/$SUBJ//"`
  xfilter "reformail -i'Subject: $SUBJECT'"
}

if ( $FIXLINKS )
{
  xfilter fixlinks
}

if ( $LIST )
{
  FOLDER=Lists.$LIST/
  include $DELIVER
}
